{
    "name": "bestsite.az/laravel-nova-double-field",
    "description": "A Laravel Nova Double field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Bestsite\\Double\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bestsite\\Double\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
